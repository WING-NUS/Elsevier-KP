mi></lnsm:mml_mrow></lnsm:mml_msup></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mn>1</lnsm:mml_mn><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mn>2</lnsm:mml_mn></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mi>&#x3ba;</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msup></lnsm:mml_mrow></lnsm:mml_mfrac></lnsm:mml_math></lnsm:mmlmath></lnsm:mml><lnsm:mml render="n" id="mml66"><refpt id="mml66"/><inl width="439" height="123" size="3748" uoi="B6VCF-52KW0RB-1-2B"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si66.gif"><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>E</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>&#x3be;</lnsm:mml_mi><lnsm:mml_mtext>,</lnsm:mml_mtext><lnsm:mml_mi>&#x3b1;</lnsm:mml_mi><lnsm:mml_mtext>,</lnsm:mml_mtext><lnsm:mml_mi>&#x3ba;</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msub><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi mathvariant="italic">XF</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msup><lnsm:mml_mi>I</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo>&#x2a7d;</lnsm:mml_mo><lnsm:mml_mi>h</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo>=</lnsm:mml_mo><lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow></lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_mfenced close=")" open="("><lnsm:mml_mrow><lnsm:mml_mi>&#x3be;</lnsm:mml_mi><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_mi>&#x3b1;</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>&#x3ba;</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_mfrac></lnsm:mml_mrow></lnsm:mml_mfenced></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mfenced close=")" open="("><lnsm:mml_mrow><lnsm:mml_mn>1</lnsm:mml_mn><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mi>&#x393;</lnsm:mml_mi><lnsm:mml_mrow><lnsm:mml_mfenced close=")" open="("><lnsm:mml_mrow><lnsm:mml_mn>1</lnsm:mml_mn><lnsm:mml_mtext>,</lnsm:mml_mtext><lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mi>h</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&#x2032;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msup></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow></lnsm:mml_mfrac></lnsm:mml_mrow></lnsm:mml_mfenced></lnsm:mml_mrow></lnsm:mml_mrow></lnsm:mml_mfenced></lnsm:mml_mrow><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_mi>&#x3b1;</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>&#x3ba;</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_mfrac><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mfenced close=")" open="("><lnsm:mml_mrow><lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow></lnsm:mml_mfrac></lnsm:mml_mrow></lnsm:mml_mfenced></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>&#x3ba;</lnsm:mml_mi><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow></lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mfenced close=")" open="("><lnsm:mml_mrow><lnsm:mml_mn>1</lnsm:mml_mn><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mi>&#x393;</lnsm:mml_mi><lnsm:mml_mrow><lnsm:mml_mfenced close=")" open="("><lnsm:mml_mrow><lnsm:mml_mi>&#x3ba;</lnsm:mml_mi><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn><lnsm:mml_mtext>,</lnsm:mml_mtext><lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mi>h</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&#x2032;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msup></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow></lnsm:mml_mfrac></lnsm:mml_mrow></lnsm:mml_mfenced></lnsm:mml_mrow></lnsm:mml_mrow></lnsm:mml_mfenced></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml></lnv:MML-LIST><lnv:TEXT-1><lnsm:view type="all"><refpt id="s0005"/><h l="1">1. Introduction</h></lnsm:view><lnsm:view type="all"/><refpt id="p0005"/><p>In flood frequency analysis, it is popular to use annual maximum series, assuming their interannual independence and stationarity. The annual maximum series of floods can be extracted from systematic and/or historical data. While the systematic data are gauged at pre-designed sites and they are collected and recorded systematically, historical data from periods prior to the systematic record can be obtained from written records, historical documents, high water marks, geomorphological evidence, and sometimes, from public verbal communication <refpt id="bb0005"/><lk refid="b0005">&#x5b;1&#x5d;</lk>. Since the length of systematic data is often not significant enough to make a reliable estimation in statistics, the value of historical information in the analysis of flood frequency has long been recognized <refpt id="bb0125"/><lk refid="b0125">&#x5b;25&#x5d;</lk> and <refpt id="bb0120"/><lk refid="b0120">&#x5b;24&#x5d;</lk>. However, historical data are often censored when only major floods have been recorded <refpt id="bb0035"/><lk refid="b0035">&#x5b;7&#x5d;</lk>; therefore many studies have dealt with the use of historical information as a censored data problem at ungauged sites <refpt id="bb0135"/><lk refid="b0135">&#x5b;27&#x5d;</lk>, <refpt id="bb0070"/><lk refid="b0070">&#x5b;14&#x5d;</lk>, <refpt id="bb0095"/><lk refid="b0095">&#x5b;19&#x5d;</lk>, <refpt id="bb0040"/><lk refid="b0040">&#x5b;8&#x5d;</lk>, <refpt id="bb0115"/><lk refid="b0115">&#x5b;23&#x5d;</lk>, <refpt id="bb0025"/><lk refid="b0025">&#x5b;5&#x5d;</lk> and <refpt id="bb0020"/><lk refid="b0020">&#x5b;4&#x5d;</lk>&#x5d;.</p><lnsm:view type="all"><refpt id="p0010"/><p>A milestone in the practical use of historical information in flood frequency analysis can be found in Bulletin 17B (B17B) <refpt id="bb0100"/><lk refid="b0100">&#x5b;20&#x5d;</lk> in which the United States Water Resources Council recommended weighted moments for the Log Pearson Type III (LP3) distribution. As a Method of Moment (MOM) estimator for censored data, B17B adjusts the LP3 moments based on the weights that are determined by the ratio of the lengths of the systematic and historical data in the censored range. Note that the Bulletin 17B contains various features such as low-outlier rejection and skewness estimates from a map and thus the abbreviation of B17B indicates only the MOM estimator with the same adjustments for historical data as made in the Bulletin 17B method. The B17B estimator was shown to be robust in many cases, but several studies have pointed out that it is inefficient <refpt id="bb0060"/><lk refid="b0060">&#x5b;12&#x5d;</lk>, <refpt id="bb0125"/><lk refid="b0125">&#x5b;25&#x5d;</lk>, <refpt id="bb0080"/><lk refid="b0080">&#x5b;16&#x5d;</lk> and <refpt id="bb0130"/><lk refid="b0130">&#x5b;26&#x5d;</lk>.</p></lnsm:view><lnsm:view type="all"><refpt id="p0015"/><p>Cohn et al. <refpt id="bb0040"/><lk refid="b0040">&#x5b;8&#x5d;</lk> revisited the moment-based procedure and improved the B17B estimator in order to propose the expected moments algorithm (EMA); they compared the performance of the proposed estimator in quantile estimation with other estimators for the LP3 distribution. In contrast with the B17B estimator where sample moments are used for the censored data, the EMA uses parametric equations based on the conditional expectation. The probability weighted moment (PWM) was also investigated for the censored data problem by Wang <refpt id="bb0135"/><lk refid="b0135">&#x5b;27&#x5d;</lk>. He introduced an unbiased estimator based on PWM for censored data, called the partial PWM estimator (PPWM). Hosking <refpt id="bb0095"/><lk refid="b0095">&#x5b;19&#x5d;</lk> proposed a similar estimator for censored data and Zafrakou-Koulouris et al. <refpt id="bb0140"/><lk refid="b0140">&#x5b;28&#x5d;</lk> demonstrated the relationship between these two estimators. An alternative to the MOM and PWM estimators is the maximum likelihood (ML) estimator. Condie and Lee <refpt id="bb0060"/><lk refid="b0060">&#x5b;12&#x5d;</lk> and Stedinger and Cohn <refpt id="bb0125"/><lk refid="b0125">&#x5b;25&#x5d;</lk> showed that the ML estimator for the lognormal distribution is more efficient and practically more applicable to real data analysis than the B17B estimator is. However, the ML estimator does not always work well. That is, it may perform poorly when only a small sample of data is available or there is an outlier. ML estimation may also perform poorly when the model is misspecified <refpt id="bb0015"/><lk refid="b0015">&#x5b;3&#x5d;</lk>, <refpt id="bb0085"/><lk refid="b0085">&#x5b;17&#x5d;</lk> and <refpt id="bb0045"/><lk refid="b0045">&#x5b;9&#x5d;</lk>. To improve the ML estimator in the generalized extreme value (GEV) distribution, Coles and Dixon <refpt id="bb0055"/><lk refid="b0055">&#x5b;11&#x5d;</lk> proposed a penalized ML estimator which stems from Bayesian estimator with prior information. The estimator corrects the bias in the estimation of the shape parameter with a penalty function. This study employed the penalized ML (called pML hereafter) estimator which Coles and Dixon <refpt id="bb0055"/><lk refid="b0055">&#x5b;11&#x5d;</lk> proposed because Coles and Dixon <refpt id="bb0055"/><lk refid="b0055">&#x5b;11&#x5d;</lk> argued that the improved behavior of PWM over the ML estimator (e.g. <refpt id="bb0090"/><lk refid="b0090">&#x5b;18&#x5d;</lk> is largely an artifact of the restriction of the GEV shape parameter that is needed to make PWM work and that the ML estimator performs at least as well as PWM, when a similar restriction is applied to the ML estimator.</p></lnsm:view><lnsm:view type="all"><refpt id="p0020"/><p>In this paper, we propose a new estimator called the expected probability weighted moment (EPWM) estimator for censored data. The EPWM estimator, an extension of the PPWM estimator <refpt id="bb0135"/><lk refid="b0135">&#x5b;27&#x5d;</lk>, utilizes parametric equations as the EMA does. We apply the PPWM estimator to the GEV distribution in order to demonstrate the superiority of the EPWM estimator to both the pML and other moment-based estimators. <refpt id="bt0005"/><lk refid="t0005">Table 1</lk> summarizes the four moment-based estimators for censored data.</p><lnsm:tbl loc="float"><refpt id="t0005"/><lnsm:tbllabel>Table 1. </lnsm:tbllabel><lnsm:caption><lnsm:view type="all"><refpt id="sp080"/><lnsm:p>Four moment-based estimators developed for censored flood data.</lnsm:p></lnsm:view></lnsm:caption><table rowsep="0" colsep="0" frame="topbot"><tgroup cols="3"><colspec colname="col1" align="left"/><colspec colname="col2" align="left"/><colspec colname="col3" align="left"/><thead><row valign="top" rowsep="1"><entry valign="center" align="left"/><entry valign="center" align="left">Sample moment equation</entry><entry valign="center" align="left">Parametric equation</entry></row></thead><tbody><row valign="top"><entry valign="center" align="left">Product moment</entry><entry valign="center" align="left">B17B estimator <refpt id="bb0100"/><lk refid="b0100">&#x5b;20&#x5d;</lk></entry><entry valign="center" align="left">Expected moments algorithm <refpt id="bb0040"/><lk refid="b0040">&#x5b;8&#x5d;</lk></entry></row><row valign="top"><entry valign="center" align="left">Probability weighted moment</entry><entry valign="center" align="left">PPWM estimator <refpt id="bb0135"/><lk refid="b0135">&#x5b;27&#x5d;</lk></entry><entry valign="center" align="left">This study</entry></row></tbody></tgroup></table></lnsm:tbl></lnsm:view><lnsm:view type="all"><refpt id="p0025"/><p>This paper is organized as follows. In Section <lk refid="s0010">2</lk>, we first summarize the derivation of the B17B and PPWM estimators for censored data and show that the EMA estimator is an extension of the B17B estimator when parametric equations are used. The EPWM estimator is then proposed by incorporating parametric equations into the PWM estimator. Section <lk refid="s0025">3</lk> derives three parameters of the GEV distribution with five estimators such as the four moment-based estimators in <refpt id="bt0005"/><lk refid="t0005">Table 1</lk> and the pML estimator. Section <lk refid="s0045">4</lk> compares the five estimators numerically using a Monte Carlo simulation. The conclusion and the future research are addressed in Section <lk refid="s0070">5</lk>.</p></lnsm:view><lnsm:view type="all"><refpt id="s0010"/><h l="1">2. Moment-based estimator for censored data</h></lnsm:view><lnsm:view type="all"><refpt id="p0030"/><p>In this section, we review various moment-based estimators for censored data and propose a new estimator based on PWMs. Here we only consider type-I left censoring, in which the values of observations below a given threshold <emph typestyle="it">h</emph> are not observed. Let <emph typestyle="it">N</emph><sub loc="post"><emph typestyle="it">H</emph></sub>, <emph typestyle="it">N</emph><sub loc="post"><emph typestyle="it">S</emph></sub> and <emph typestyle="it">N</emph>(=<emph typestyle="it">N</emph><sub loc="post"><emph typestyle="it">H</emph></sub><lnsm:hsp sp="0.25"/>+<lnsm:hsp sp="0.25"/><emph typestyle="it">N</emph><sub loc="post"><emph typestyle="it">S</emph></sub>) be the lengths of the historical data, the systematic data, and the entire data, respectively. The superscripts &lt; and &gt; stand for &#x2018;below&#x2019; and &#x2018;above&#x2019; the threshold, respectively. For example, <lnsm:mml render="n" id="mml1"><refpt id="mml1"/><inl width="24" height="18" size="278" uoi="B6VCF-52KW0RB-1-1"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si1.gif"><lnsm:mml_mrow><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>N</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>H</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&lt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml> is the number of observations that are below the threshold in the historical data. In the same manner, we denote the random variables <lnsm:mml render="n" id="mml2"><refpt id="mml2"/><inl width="23" height="19" size="264" uoi="B6VCF-52KW0RB-1-7"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si2.gif"><lnsm:mml_mrow><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>X</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>S</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&lt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml> and <lnsm:mml render="n" id="mml3"><refpt id="mml3"/><inl width="23" height="19" size="262" uoi="B6VCF-52KW0RB-1-B"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si3.gif"><lnsm:mml_mrow><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>X</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>S</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&gt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml> as annual peak floods less than the threshold and greater than the threshold in the systematic data, respectively, and <lnsm:mml render="n" id="mml4"><refpt id="mml4"/><inl width="24" height="18" size="275" uoi="B6VCF-52KW0RB-1-K"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si4.gif"><lnsm:mml_mrow><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>X</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>H</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&lt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml> and <lnsm:mml render="n" id="mml5"><refpt id="mml5"/><inl width="24" height="18" size="270" uoi="B6VCF-52KW0RB-1-S"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si5.gif"><lnsm:mml_mrow><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>X</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>H</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&gt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml> are defined similarly. <lnsm:mml render="n" id="mml6"><refpt id="mml6"/><inl width="78" height="19" size="444" uoi="B6VCF-52KW0RB-1-X"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si6.gif"><lnsm:mml_mrow><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>S</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&lt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup><lnsm:mml_mtext>,</lnsm:mml_mtext><lnsm:mml_mspace width="0.35em"/><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>S</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&gt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup><lnsm:mml_mtext>,</lnsm:mml_mtext><lnsm:mml_mspace width="0.35em"/><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>H</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&lt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml> and <lnsm:mml render="n" id="mml7"><refpt id="mml7"/><inl width="19" height="17" size="249" uoi="B6VCF-52KW0RB-1-Y"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si7.gif"><lnsm:mml_mrow><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>H</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&lt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml> represent their observations. Note that we do not observe <lnsm:mml render="n" id="mml8"><refpt id="mml8"/><inl width="19" height="17" size="249" uoi="B6VCF-52KW0RB-1-10"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si8.gif"><lnsm:mml_mrow><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>H</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo>&lt;</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msubsup></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml>. No superscript is used when they represent data both below and above the threshold.</p></lnsm:view><lnsm:view type="all"><refpt id="s0015"/><h l="2">2.1. Use of sample moment equation</h></lnsm:view><lnsm:view type="all"><refpt id="p0035"/><p>Let <emph typestyle="it">f</emph>(<emph typestyle="it">x</emph><lnsm:hsp sp="0.12"/>:<lnsm:hsp sp="0.12"/><emph typestyle="it">&#x3b8;</emph>) be the probability density function of annual peak flood <emph typestyle="it">X</emph>, where <emph typestyle="it">&#x3b8;</emph><lnsm:hsp sp="0.25"/>&#x2208;<lnsm:hsp sp="0.25"/><emph typestyle="it">&#x398;</emph><lnsm:hsp sp="0.25"/>&#x2282;<lnsm:hsp sp="0.25"/><emph typestyle="it">R</emph><sup loc="post"><emph typestyle="it">p</emph></sup> stands for parameters of interest. We assume that the observations <emph typestyle="it">x</emph><sub loc="post">1</sub>,<lnsm:hsp sp="0.12"/>&#x2026;<lnsm:hsp sp="0.12"/>,<lnsm:hsp sp="0.12"/><emph typestyle="it">x</emph><sub loc="post"><emph typestyle="it">N</emph></sub> are independent realizations of <emph typestyle="it">X</emph>. A general procedure for obtaining moment-based estimators with complete observations <emph typestyle="it">x</emph><sub loc="post">1</sub>,<lnsm:hsp sp="0.12"/>&#x2026;<lnsm:hsp sp="0.12"/>,<lnsm:hsp sp="0.12"/><emph typestyle="it">x</emph><sub loc="post"><emph typestyle="it">N</emph></sub> is followed in order to solve the following equation:<lnsm:fd render="n" id="1"><refpt id="e0005"/><lnsm:mml render="y" id="mml9"><refpt id="mml9"/><lnsm:textbox><emph typestyle="it">E</emph><sub><emph typestyle="it">&#x3b8;</emph></sub>(<emph typestyle="it">g</emph><sub><emph typestyle="it">r</emph></sub>(<emph typestyle="it">X</emph>))=<emph typestyle="it">&#x3d5;</emph><sub><emph typestyle="it">r</emph></sub>(<emph typestyle="it">x</emph><sub>1</sub>,&#x2026;,<emph typestyle="it">x</emph><sub><emph typestyle="it">N</emph></sub>)</lnsm:textbox><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si9.gif"><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>E</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>&#x3b8;</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msub><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>g</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msub><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo>=</lnsm:mml_mo><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>&#x3d5;</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msub><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow></lnsm:mml_msub><lnsm:mml_mtext>,</lnsm:mml_mtext><lnsm:mml_mo>&#x2026;</lnsm:mml_mo><lnsm:mml_mtext>,</lnsm:mml_mtext><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>N</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msub><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo></lnsm:mml_math></lnsm:mmlmath></lnsm:mml></lnsm:fd>for predefined function <emph typestyle="it">h</emph><sub loc="post"><emph typestyle="it">r</emph></sub> and unbiased estimators <emph typestyle="it">&#x3d5;</emph><sub loc="post"><emph typestyle="it">r</emph></sub>(<emph typestyle="it">x</emph><sub loc="post">1</sub>,<lnsm:hsp sp="0.12"/>&#x2026;<lnsm:hsp sp="0.12"/>,<lnsm:hsp sp="0.12"/><emph typestyle="it">x</emph><sub loc="post"><emph typestyle="it">N</emph></sub>) of <emph typestyle="it">E</emph><sub loc="post"><emph typestyle="it">&#x3b8;</emph></sub>(<emph typestyle="it">g</emph><sub loc="post"><emph typestyle="it">r</emph></sub>(<emph typestyle="it">X</emph>)) for <emph typestyle="it">r</emph><lnsm:hsp sp="0.25"/>=<lnsm:hsp sp="0.25"/>1,<lnsm:hsp sp="0.12"/>&#x2026;<lnsm:hsp sp="0.12"/>,<lnsm:hsp sp="0.12"/><emph typestyle="it">p</emph>. When <emph typestyle="it">g</emph><sub loc="post"><emph typestyle="it">r</emph></sub>(<emph typestyle="it">x</emph>)<lnsm:hsp sp="0.25"/>=<lnsm:hsp sp="0.25"/><emph typestyle="it">x</emph><sup loc="post"><emph typestyle="it">r</emph></sup> and <lnsm:mml render="n" id="mml10"><refpt id="mml10"/><inl width="107" height="21" size="592" uoi="B6VCF-52KW0RB-1-2"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si10.gif"><lnsm:mml_mrow><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>&#x3d5;</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msub><lnsm:mml_mo>=</lnsm:mml_mo><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mo>&#x2211;</lnsm:mml_mo></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>i</lnsm:mml_mi><lnsm:mml_mo>=</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>N</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msubsup><lnsm:mml_msubsup><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>i</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msubsup><lnsm:mml_mo>/</lnsm:mml_mo><lnsm:mml_mi>N</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_math></lnsm:mmlmath></lnsm:mml>, the estimator is called a MOM estimator. If <emph typestyle="it">g</emph><sub loc="post"><emph typestyle="it">r</emph></sub>(<emph typestyle="it">x</emph>)<lnsm:hsp sp="0.25"/>=<lnsm:hsp sp="0.25"/><emph typestyle="it">xF</emph><sub loc="post"><emph typestyle="it">&#x3b8;</emph></sub>(<emph typestyle="it">x</emph>)<sup loc="post"><emph typestyle="it">r</emph></sup>, where <emph typestyle="it">F</emph><sub loc="post"><emph typestyle="it">&#x3b8;</emph></sub> is the distribution function of <emph typestyle="it">f</emph>(<emph typestyle="it">x</emph><lnsm:hsp sp="0.12"/>:<lnsm:hsp sp="0.12"/><emph typestyle="it">&#x3b8;</emph>), and <emph typestyle="it">&#x3d5;</emph><sub loc="post"><emph typestyle="it">r</emph></sub> is given as<lnsm:fd render="n" id="2"><refpt id="e0010"/><lnsm:mml render="n" id="mml11"><refpt id="mml11"/><inl width="303" height="48" size="1827" uoi="B6VCF-52KW0RB-1-12"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si11.gif"><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>&#x3d5;</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msub><lnsm:mml_mo>=</lnsm:mml_mo><lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>N</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_mfrac><lnsm:mml_munderover><lnsm:mml_mrow><lnsm:mml_mo>&#x2211;</lnsm:mml_mo></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>i</lnsm:mml_mi><lnsm:mml_mo>=</lnsm:mml_mo><lnsm:mml_mi>r</lnsm:mml_mi><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>N</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_munderover><lnsm:mml_mfrac><lnsm:mml_mrow><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>i</lnsm:mml_mi><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>i</lnsm:mml_mi><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mn>2</lnsm:mml_mn><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo>&#x22ef;</lnsm:mml_mo><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>i</lnsm:mml_mi><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mi>r</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>N</lnsm:mml_mi><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mn>1</lnsm:mml_mn><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>N</lnsm:mml_mi><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mn>2</lnsm:mml_mn><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo>&#x22ef;</lnsm:mml_mo><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>N</lnsm:mml_mi><lnsm:mml_mo>-</lnsm:mml_mo><lnsm:mml_mi>r</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_mfrac><lnsm:mml_msub><lnsm:mml_mrow><lnsm:mml_mi>x</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>i</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo></lnsm:mml_mrow></lnsm:mml_msub></lnsm:mml_math></lnsm:mmlmath></lnsm:mml></lnsm:fd>where <emph typestyle="it">x</emph><sub loc="post">(<emph typestyle="it">i</emph>)</sub>&#x2019;s are the order statistics of the observations, then the estimate becomes the PWM estimator proposed by Greenwood et al. <refpt id="bb0075"/><lk refid="b0075">&#x5b;15&#x5d;</lk>. If the parameters of a distribution are uniquely determined by their moments and the mapping between the moments and the parameters is continuous, then the moment-based estimator is consistent, i.e. the estimator converges to a true value as the number of samples goes to infinity. Consistency is a substantial property required for an estimator. Moreover, moment-based estimators are relatively easy to compute in general and thus have been popularly used in the flood analysis.</p></lnsm:view><lnsm:view type="all"><refpt id="p0040"/><p>When we have censored observations, we need to adjust <emph typestyle="it">&#x3d5;</emph><sub loc="post"><emph typestyle="it">r</emph></sub> appropriately in order to obtain moment-based estimators. For the MOM estimator, we decompose <emph typestyle="it">EX</emph><sup loc="post"><emph typestyle="it">r</emph></sup> as<lnsm:fd render="n" id="3a"><refpt id="e0015"/><lnsm:mml render="y" id="mml12"><refpt id="mml12"/><lnsm:textbox><emph typestyle="it">EX</emph><sup><emph typestyle="it">r</emph></sup>=<emph typestyle="it">EX</emph><sup><emph typestyle="it">r</emph></sup><emph typestyle="it">I</emph>(<emph typestyle="it">X</emph>&#x2a7d;<emph typestyle="it">h</emph>)+<emph typestyle="it">EX</emph><sup><emph typestyle="it">r</emph></sup><emph typestyle="it">I</emph>(<emph typestyle="it">X</emph>&gt;<emph typestyle="it">h</emph>)=<emph typestyle="it">P</emph>(<emph typestyle="it">X</emph>&#x2a7d;<emph typestyle="it">h</emph>)<emph typestyle="it">E</emph>(<emph typestyle="it">X</emph><sup><emph typestyle="it">r</emph></sup>&#x7c;<emph typestyle="it">X</emph>&#x2a7d;<emph typestyle="it">h</emph>)+<emph typestyle="it">P</emph>(<emph typestyle="it">X</emph>&gt;<emph typestyle="it">h</emph>)<emph typestyle="it">E</emph>(<emph typestyle="it">X</emph><sup><emph typestyle="it">r</emph></sup>&#x7c;<emph typestyle="it">X</emph>&gt;<emph typestyle="it">h</emph>)</lnsm:textbox><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si12.gif"><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mi mathvariant="italic">EX</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msup><lnsm:mml_mo>=</lnsm:mml_mo><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mi mathvariant="italic">EX</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msup><lnsm:mml_mi>I</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo>&#x2a7d;</lnsm:mml_mo><lnsm:mml_mi>h</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mi mathvariant="italic">EX</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msup><lnsm:mml_mi>I</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo>&gt;</lnsm:mml_mo><lnsm:mml_mi>h</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo>=</lnsm:mml_mo><lnsm:mml_mi>P</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo>&#x2a7d;</lnsm:mml_mo><lnsm:mml_mi>h</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mi>E</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mi>X</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msup><lnsm:mml_mo stretchy="false">&#x7c;</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo>&#x2a7d;</lnsm:mml_mo><lnsm:mml_mi>h</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo>+</lnsm:mml_mo><lnsm:mml_mi>P</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo>&gt;</lnsm:mml_mo><lnsm:mml_mi>h</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mi>E</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_msup><lnsm:mml_mrow><lnsm:mml_mi>X</lnsm:mml_mi></lnsm:mml_mrow><lnsm:mml_mrow><lnsm:mml_mi>r</lnsm:mml_mi></lnsm:mml_mrow></lnsm:mml_msup><lnsm:mml_mo stretchy="false">&#x7c;</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo>&gt;</lnsm:mml_mo><lnsm:mml_mi>h</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo></lnsm:mml_math></lnsm:mmlmath></lnsm:mml></lnsm:fd>where<lnsm:fd render="n" id="3b"><refpt id="e0020"/><lnsm:mml render="n" id="mml13"><refpt id="mml13"/><inl width="202" height="42" size="1203" uoi="B6VCF-52KW0RB-1-14"/><lnsm:mmlmath><lnsm:mml_math overflow="scroll" altimg="si13.gif"><lnsm:mml_mi>I</lnsm:mml_mi><lnsm:mml_mo stretchy="false">(</lnsm:mml_mo><lnsm:mml_mi>X</lnsm:mml_mi><lnsm:mml_mo>&#x2a7d;</lnsm:mml_mo><lnsm:mml_mi>h</lnsm:mml_mi><lnsm:mml_mo stretchy="false">)</lnsm:mml_mo><lnsm:mml_mo>=</lnsm:mml_mo>